{
  "name": "assembl-desktop",
  "version": "0.0.4",
  "description": "Assembl Desktop allows you to transmit files directly to another person.",
  "homepage": "https://assembl.science/",
  "author": "Assembl Inc.",
  "license": "SEE LICENSE IN LICENSE.MD",
  "main": "main.js",
  "build": {
    "appId": "science.assembl.desktop",
    "productName": "Assembl Desktop",
    "copyright": "Copyright Â© 2019 Assembl Inc.",
    "extraFiles": [
      "resources/*",
      "LICENSE.MD"
    ],
    "fileAssociations": [
      {
        "ext": "astfv",
        "name": "Assembl Transfer Validation",
        "description": "The validation file for a file transfer done via Assembl",
        "mimeType": "application/assembl-transfer-validation",
        "role": "Viewer",
        "isPackage": false
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "build-win": "build --win",
    "pack": "electron-builder"
  },
  "devDependencies": {
    "electron": "^4.0.3",
    "electron-builder": "^20.40.2"
  },
  "dependencies": {
    "@nodeutils/defaults-deep": "^1.1.0",
    "crypto": "^1.0.1",
    "electron-context-menu": "^0.10.1",
    "js-sha3": "^0.8.0",
    "kafkajs": "^1.8.0",
    "kbpgp": "^2.0.82",
    "multiaddr": "^6.0.4",
    "mv": "^2.1.1",
    "nat-upnp": "^1.1.1",
    "node": "^11.9.0",
    "peer-id": "^0.12.2",
    "peer-info": "^0.15.1",
    "prettysize": "^2.0.0",
    "pull-stream": "^3.6.9",
    "socket.io": "^2.2.0",
    "socket.io-stream": "^0.9.1",
    "ssh2": "^0.8.4",
    "stellar-base": "^0.10.0",
    "stellar-sdk": "^0.12.0"
  },
  "engines": {
    "node": "11.6.0",
    "npm": "5.5.1"
  }
}
